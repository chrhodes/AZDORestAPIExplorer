                                            ToolTip="{Binding GetPullRequestCommitsToolTip, FallbackValue='Get PR Commits ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestIterationsCommand }"
                                            Content="{Binding GetPullRequestIterationsContent, FallbackValue='Get PR Iterations' }"
                                            ToolTip="{Binding GetPullRequestIterationsToolTip, FallbackValue='Get PR Iterations ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestLabelsCommand }"
                                            Content="{Binding GetPullRequestLabelsContent, FallbackValue='Get PR Labels' }"
                                            ToolTip="{Binding GetPullRequestLabelsToolTip, FallbackValue='Get PR Labels ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestPropertiesCommand }"
                                            Content="{Binding GetPullRequestPropertiesContent, FallbackValue='Get PR Properties' }"
                                            ToolTip="{Binding GetPullRequestPropertiesToolTip, FallbackValue='Get PR Properties ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestReviewersCommand }"
                                            Content="{Binding GetPullRequestReviewersContent, FallbackValue='Get PR Reviewers' }"
                                            ToolTip="{Binding GetPullRequestReviewersToolTip, FallbackValue='Get PR Reviewers ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestStatusesCommand }"
                                            Content="{Binding GetPullRequestStatusesContent, FallbackValue='Get PR Statuses' }"
                                            ToolTip="{Binding GetPullRequestStatusesToolTip, FallbackValue='Get PR Statuses ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestThreadsCommand }"
                                            Content="{Binding GetPullRequestThreadsContent, FallbackValue='Get PR Threads' }"
                                            ToolTip="{Binding GetPullRequestThreadsToolTip, FallbackValue='Get PR Threads ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestWorkItemsCommand }"
                                            Content="{Binding GetPullRequestWorkItemsContent, FallbackValue='Get PR WorkItems' }"
                                            ToolTip="{Binding GetPullRequestWorkItemsToolTip, FallbackValue='Get PR WorkItems ToolTip' }" />
                                    </dxlc:LayoutItem>

                                </dxlc:LayoutGroup>

                            </dxlc:LayoutGroup>

                            <dxlc:LayoutItem>
                                <Button
                                    Command="{Binding GetPushesCommand }"
                                    Content="{Binding GetPushesContent, FallbackValue='GetPushes' }"
                                    ToolTip="{Binding GetPushesToolTip, FallbackValue='GetPushes ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutItem>
                                <Button
                                    Command="{Binding GetRefsCommand }"
                                    Content="{Binding GetRefsContent, FallbackValue='Get Refs' }"
                                    ToolTip="{Binding GetRefsToolTip, FallbackValue='Get Refs ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutItem>
                                <Button
                                    Command="{Binding GetStatsCommand }"
                                    Content="{Binding GetStatsContent, FallbackValue='Get Stats' }"
                                    ToolTip="{Binding GetStatsToolTip, FallbackValue='Get Stats ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutGroup Header="Commit Level" IsCollapsed="True" IsCollapsible="True"
                                Orientation="Vertical" View="GroupBox"
                                Background="{StaticResource CommitColor}">

                                <dxlc:LayoutItem>
                                    <Button
                                        Command="{Binding GetCommitChangesCommand }"
                                        Content="{Binding GetCommitChangesContent, FallbackValue='GetCommitChanges' }"
                                        ToolTip="{Binding GetCommitChangesToolTip, FallbackValue='GetCommitChanges ToolTip' }" />
                                </dxlc:LayoutItem>
                                
                            </dxlc:LayoutGroup>
                            
                        </dxlc:LayoutGroup>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Graph *" IsCollapsible="True" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}"
                             Background="{StaticResource OrganizationColor}">

                         <dxlc:LayoutItem>
                             <Button 
 		                        Command="{Binding GetGroupsCommand }"
 		                        Content="{Binding GetGroupsContent, FallbackValue='GetGroups' }"
 		                        ToolTip="{Binding GetGroupsToolTip, FallbackValue='GetGroups ToolTip' }" />
                         </dxlc:LayoutItem>
                        
                        <dxlc:LayoutItem>
                            <Button 
                        		Command="{Binding GetUsersCommand }"
                        		Content="{Binding GetUsersContent, FallbackValue='GetUsers' }"
                        		ToolTip="{Binding GetUsersToolTip, FallbackValue='GetUsers ToolTip' }" />
                        </dxlc:LayoutItem>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Identities *" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">
                        <TextBlock Text="Not Implemented Yet" />
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Member Entitlement Management *" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">
                        <TextBlock Text="Not Implemented Yet" />
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Notification *" IsCollapsed="True"
