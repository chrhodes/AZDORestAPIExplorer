<vncmvvm:ViewBase
    x:Class="AZDORestApiExplorer.Presentation.Views.CommandsGet"
    x:Name="thisCommandsGet"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:prism="http://prismlibrary.com/"
    xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
    xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vncmvvm="clr-namespace:VNC.Core.Mvvm;assembly=VNC.Core"
    mc:Ignorable="d"
    prism:ViewModelLocator.AutoWireViewModel="False"

    d:DesignStyle="{StaticResource vncDesignStyleUC}"
>
    <d:DesignerProperties.DesignStyle>
        <Style TargetType="UserControl">
            <Setter Property="Background" Value="White" />
        </Style>
    </d:DesignerProperties.DesignStyle>

    <UserControl.Resources />

    <dxlc:LayoutControl
            HorizontalAlignment="Stretch"
            VerticalAlignment="Stretch"
            Background="Transparent"
            Name="lc_Root"
            Orientation="Vertical">

        <dxlc:LayoutGroup Orientation="Vertical">

            <dxlc:LayoutGroup Orientation="Horizontal">

                <dxlc:LayoutGroup Orientation="Vertical">

                    <dxlc:LayoutGroup Header="Core - Organization Level" IsCollapsed="False"
                        Style="{StaticResource GetLayoutGroup}"
                        Background="{StaticResource OrganizationColor}">

                        <dxlc:LayoutItem>
                            <Button
                                Command="{Binding GetCoreProcessesCommand }"
                                Content="{Binding GetCoreProcessesContent, FallbackValue='Get Processes' }"
                                ToolTip="{Binding GetCoreProcessesToolTip, FallbackValue='Get Processes ToolTip' }" />
                        </dxlc:LayoutItem>

                        <dxlc:LayoutItem>
                            <Button
                                Command="{Binding GetProjectsCommand }"
                                Content="{Binding GetProjectsContent, FallbackValue='Get Projects' }"
                                ToolTip="{Binding GetProjectsToolTip, FallbackValue='Get Projects ToolTip' }" />
                        </dxlc:LayoutItem>

                        <dxlc:LayoutItem>
                            <Button
                                Command="{Binding GetTeamsCommand }"
                                Content="{Binding GetTeamsContent, FallbackValue='Get Teams' }"
                                ToolTip="{Binding GetTeamsToolTip, FallbackValue='Get Teams ToolTip' }" />
                        </dxlc:LayoutItem>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Accounts - Organization Level" IsCollapsed="True"
                        Style="{StaticResource GetLayoutGroup}"
                        Background="{StaticResource OrganizationColor}">

                        <dxlc:LayoutItem>
                            <Button
                                Command="{Binding GetAccountsCommand }"
                                Content="{Binding GetAccountsContent, FallbackValue='Get Accounts' }"
                                ToolTip="{Binding GetAccountsToolTip, FallbackValue='Get Accounts ToolTip' }" />
                        </dxlc:LayoutItem>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Approvals and Checks *" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">
                        <TextBlock Text="Not Implemented Yet" />
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Artifacts" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}"
                             Background="{StaticResource OrganizationColor}">

                        <dxlc:LayoutItem>
                            <Button
 		                        Command="{Binding GetFeedsCommand }"
 		                        Content="{Binding GetFeedsContent, FallbackValue='GetFeeds' }"
 		                        ToolTip="{Binding GetFeedsToolTip, FallbackValue='GetFeeds ToolTip' }" />
                        </dxlc:LayoutItem>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Artifacts Package Types *" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">
                        <TextBlock Text="Not Implemented Yet" />
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Audit *" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">
                        <TextBlock Text="Not Implemented Yet" />
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Build - Organization Level" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}">

                        <dxlc:LayoutItem>
                            <Button 
                        		Command="{Binding GetControllersCommand }"
                        		Content="{Binding GetControllersContent, FallbackValue='GetControllers' }"
                        		ToolTip="{Binding GetControllersToolTip, FallbackValue='GetControllers ToolTip' }" />
                        </dxlc:LayoutItem>

                        <dxlc:LayoutGroup Header="Project Level" IsCollapsed="False"
                            Orientation="Vertical" View="GroupBox"
                            Background="{StaticResource ProjectColor}">

                            <dxlc:LayoutItem>
                                <Button 
                            		Command="{Binding GetAuthorizedResourcesCommand }"
                            		Content="{Binding GetAuthorizedResourcesContent, FallbackValue='GetAuthorizedResources' }"
                            		ToolTip="{Binding GetAuthorizedResourcesToolTip, FallbackValue='GetAuthorizedResources ToolTip' }" />
                            </dxlc:LayoutItem>
                            
                            <dxlc:LayoutItem>
                                <Button 
                            		Command="{Binding GetBuildsCommand }"
                            		Content="{Binding GetBuildsContent, FallbackValue='GetBuilds' }"
                            		ToolTip="{Binding GetBuildsToolTip, FallbackValue='GetBuilds ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutItem>
                                <Button 
		                            Command="{Binding GetDefinitionsCommand }"
		                            Content="{Binding GetDefinitionsContent, FallbackValue='GetDefinitions' }"
		                            ToolTip="{Binding GetDefinitionsToolTip, FallbackValue='GetDefinitions ToolTip' }" />
                            </dxlc:LayoutItem>
                            
                            <dxlc:LayoutItem>
                                <Button 
                            		Command="{Binding GetGeneralSettingsCommand }"
                            		Content="{Binding GetGeneralSettingsContent, FallbackValue='GetGeneralSettings' }"
                            		ToolTip="{Binding GetGeneralSettingsToolTip, FallbackValue='GetGeneralSettings ToolTip' }" />
                            </dxlc:LayoutItem>
 
                            <dxlc:LayoutItem>
                                <Button 
                            		Command="{Binding GetOptionsCommand }"
                            		Content="{Binding GetOptionsContent, FallbackValue='GetOptions' }"
                            		ToolTip="{Binding GetOptionsToolTip, FallbackValue='GetOptions ToolTip' }" />
                            </dxlc:LayoutItem>
                            
