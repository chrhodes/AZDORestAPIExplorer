                                    Content="{Binding GetImportRequestsContent, FallbackValue='GetImportRequests' }"
                                    ToolTip="{Binding GetImportRequestsToolTip, FallbackValue='GetImportRequests ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutGroup Header="Items" IsCollapsed="True" IsCollapsible="True"
                                Orientation="Vertical" View="GroupBox"
                                Background="{StaticResource RepositoryColor}">

                                <dxlc:LayoutItem Label="ScopePath">
                                    <dxe:TextEdit
                                		Text="{Binding ScopePath }"
                                    	ToolTip="{Binding ScopePathToolTip }">
                                    </dxe:TextEdit>
                                </dxlc:LayoutItem>

                                <dxlc:LayoutItem Label="RecursionLevel"
                                 	LabelPosition="Left">
                                    <dxe:ComboBoxEdit x:Name="cbeRecursionLevel"
                                        EditValue="{Binding
                                        	Path=RecursionLevel,
                                            Mode=TwoWay,
                                            UpdateSourceTrigger=PropertyChanged }">
                                        <dxe:ComboBoxEdit.StyleSettings>
                                            <!--<dxe:CheckedTokenComboBoxStyleSettings />-->
                                            <!--<dxe:CheckedComboBoxStyleSettings />-->
                                            <dxe:RadioComboBoxStyleSettings />
                                            <!--<dxe:RadioTokenComboBoxStyleSettings />-->
                                            <!--<dxe:TokenComboBoxStyleSettings />-->
                                        </dxe:ComboBoxEdit.StyleSettings>
                                        <dxe:ComboBoxEditItem Content="None" />
                                        <dxe:ComboBoxEditItem Content="OneLevel" />
                                        <dxe:ComboBoxEditItem Content="Full" />
                                    </dxe:ComboBoxEdit>
                                </dxlc:LayoutItem>

                                <dxlc:LayoutItem>
