                                    		Content="{Binding GetTestPointsContent, FallbackValue='GetTestPoints' }"
                                    		ToolTip="{Binding GetTestPointsToolTip, FallbackValue='GetTestPoints ToolTip' }" />
                                    </dxlc:LayoutItem>

                                </dxlc:LayoutGroup>

                            </dxlc:LayoutGroup>

                        </dxlc:LayoutGroup>

                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Test Plan *" IsCollapsed="True"
