                                        ToolTip="{Binding GetDashboardsToolTip, FallbackValue='Get Dashboards ToolTip' }" />
                                </dxlc:LayoutItem>
                                
                            </dxlc:LayoutGroup>
                            
                        </dxlc:LayoutGroup>
                        
                    </dxlc:LayoutGroup>
                    
                </dxlc:LayoutGroup>

