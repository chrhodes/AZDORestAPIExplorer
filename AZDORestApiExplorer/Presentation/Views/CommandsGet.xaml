                                    Content="{Binding GetItemsContent, FallbackValue='GetItems' }"
                                    ToolTip="{Binding GetItemsToolTip, FallbackValue='GetItems ToolTip' }" />
                                </dxlc:LayoutItem>
                                
                            </dxlc:LayoutGroup>

                            <dxlc:LayoutItem>
                                <Button
                                    Command="{Binding GetMergesCommand }"
                                    Content="{Binding GetMergesContent, FallbackValue='GetMerges' }"
                                    ToolTip="{Binding GetMergesToolTip, FallbackValue='GetMerges ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutGroup Header="Pull Requests" IsCollapsed="True" IsCollapsible="True"
                                Orientation="Vertical" View="GroupBox"
                                Background="{StaticResource CommitColor}">

                                <dxlc:LayoutItem>
                                    <Button
                                        Command="{Binding GetPullRequestsCommand }"
                                        Content="{Binding GetPullRequestsContent, FallbackValue='Get PullRequests' }"
                                        ToolTip="{Binding GetPullRequestsToolTip, FallbackValue='Get PullRequests ToolTip' }" />
                                </dxlc:LayoutItem>

                                <dxlc:LayoutGroup Header="Pull Request Level" IsCollapsed="False" IsCollapsible="True"
                                    Orientation="Vertical" View="GroupBox"
                                    Background="{StaticResource PullRequestColor}">
                                    
                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetPullRequestAttachmentsCommand }"
                                            Content="{Binding GetPullRequestAttachmentsContent, FallbackValue='Get PR Attachments' }"
                                            ToolTip="{Binding GetPullRequestAttachmentsToolTip, FallbackValue='Get PR Attachments ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
