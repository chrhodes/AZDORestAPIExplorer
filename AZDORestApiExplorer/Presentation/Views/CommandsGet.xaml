                            Background="{StaticResource ProjectColor}">

                            <dxlc:LayoutItem>
                                <Button
                                    Command="{Binding GetTestPlansCommand }"
                                    Content="{Binding GetTestPlansContent, FallbackValue='Get Test Plans' }"
                                    ToolTip="{Binding GetTestPlansToolTip, FallbackValue='Get Test Plans ToolTip' }" />
                            </dxlc:LayoutItem>

                            <dxlc:LayoutGroup Header="Test Plan Level" IsCollapsed="True"
                                    Orientation="Vertical" View="GroupBox"
                                    Background="{StaticResource TestColor}">

                                <dxlc:LayoutItem>
                                    <Button
                                        Command="{Binding GetTestSuitesCommand }"
                                        Content="{Binding GetTestSuitesContent, FallbackValue='Get Test Suites' }"
                                        ToolTip="{Binding GetTestSuitesToolTip, FallbackValue='Get Test Suites ToolTip' }" />
                                </dxlc:LayoutItem>

                                <dxlc:LayoutGroup Header="Test Suite Level" IsCollapsed="True"
                                    Orientation="Vertical" View="GroupBox"
                                    Background="{StaticResource TestColor}">

                                    <dxlc:LayoutItem>
                                        <Button
                                            Command="{Binding GetTestCasesCommand }"
                                            Content="{Binding GetTestCasesContent, FallbackValue='GetTestCases' }"
                                            ToolTip="{Binding GetTestCasesToolTip, FallbackValue='GetTestCases ToolTip' }" />
                                    </dxlc:LayoutItem>

                                    <dxlc:LayoutItem>
                                        <Button 
