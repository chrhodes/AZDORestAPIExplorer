                                    Command="{Binding GetStatesWITPCommand }"
                                    Content="{Binding GetStatesWITPContent, FallbackValue='Get States (WITP)' }"
                                    ToolTip="{Binding GetStatesWITPToolTip, FallbackValue='Get States (WITP) ToolTip' }" />
                                </dxlc:LayoutItem>

                                <dxlc:LayoutItem Background="{StaticResource WorkItemTypeColor}">
                                    <Button
                                    Command="{Binding GetSystemControlsCommand }"
                                    Content="{Binding GetSystemControlsContent, FallbackValue='Get SystemControls' }"
                                    ToolTip="{Binding GetSystemControlsToolTip, FallbackValue='Get SystemControls ToolTip' }" />
                                </dxlc:LayoutItem>

                                <dxlc:LayoutItem Background="{StaticResource WorkItemTypeColor}">
                                    <Button
                                    Command="{Binding GetWorkItemTypesBehaviorsCommand }"
                                    Content="{Binding GetWorkItemTypesBehaviorsContent, FallbackValue='Get WorkItemTypes Behaviors' }"
                                    ToolTip="{Binding GetWorkItemTypesBehaviorsToolTip, FallbackValue='Get WorkItemTypes Behaviors ToolTip' }" />
                                </dxlc:LayoutItem>

                            </dxlc:LayoutGroup>

                        </dxlc:LayoutGroup>

                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Work Item Tracking Process Template" IsCollapsed="True"
                             Style="{StaticResource GetLayoutGroup}" >

                    </dxlc:LayoutGroup>

                </dxlc:LayoutGroup>

            </dxlc:LayoutGroup>

            <dxlc:LayoutGroup VerticalAlignment="Bottom">

                <StackPanel  Orientation="Horizontal" DockPanel.Dock="Left">
                    <StackPanel Orientation="Horizontal" Margin="5">
                        <Label Content="V:" />
                        <Label Content="{Binding ElementName=thisCommandsGet, Path=InstanceCountV}" />
                    </StackPanel>
                    <StackPanel Orientation="Horizontal" Margin="5">
                        <Label Content="VM:" />

                        <Label Content="{Binding InstanceCountVM}" />
                    </StackPanel>
                </StackPanel>
                
            </dxlc:LayoutGroup>
                
        </dxlc:LayoutGroup>            
        
    </dxlc:LayoutControl>
        
</vncmvvm:ViewBase>