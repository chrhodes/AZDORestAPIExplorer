                        <dxlc:LayoutGroup Header="Project Level" IsCollapsed="True"
                            Orientation="Vertical" View="GroupBox"
                            Background="{StaticResource ProjectColor}">

                            <dxlc:LayoutGroup Header="Team Level" IsCollapsed="True"
                                Orientation="Vertical" View="GroupBox"
                                Background="{StaticResource TeamColor}">

                                <dxlc:LayoutItem>
                                    <Button
