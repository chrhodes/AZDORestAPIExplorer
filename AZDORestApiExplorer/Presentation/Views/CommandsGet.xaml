                                </dxlc:LayoutItem>
                                
                            </dxlc:LayoutGroup>
                        
                        </dxlc:LayoutGroup>
                        
                    </dxlc:LayoutGroup>

                    <dxlc:LayoutGroup Header="Cloud Load Test *" IsCollapsed="True"
